server {
    
  server_name profile.havehalalwilltravel.com;
    
  location / {
    proxy_pass http://119.8.173.175:5003; # Change the port if needed
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection 'upgrade';
    proxy_set_header Host $host;
    proxy_cache_bypass $http_upgrade;
 
    add_header 'Access-Control-Allow-Origin' '*';
    add_header 'Access-Control-Allow-Credentials' 'true';
  }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/profile.havehalalwilltravel.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/profile.havehalalwilltravel.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = profile.havehalalwilltravel.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    
  server_name profile.havehalalwilltravel.com;
    listen 80;
    return 404; # managed by Certbot


}